<body class="login">

<asp:Panel ID="aspPanel" runat="server" DefaultButton="btnSubmit">
<div class="login-wrapper">

 <article class="content-box minimizer">
			<header>
				<h2>Admin Login</h2>		
			</header>
			<section>
				
				<div id="logform">
					<form id="form1" runat="server">      
                    
                    <div style="margin-bottom:10px">
                        <asp:ValidationSummary ID="vsLogin" ValidationGroup="check" runat="server" CssClass="notification error" HeaderText="Error notification" />                        
                   </div> 
                      <div style="margin-bottom:20px">             	
                         <asp:Label runat="server" ID="lblmsg" cssclass="notification error" Visible="false" ></asp:Label>
			          </div> 
						<dl>
							<dt>
								<label>Login<span >*</span></label>
							</dt>
							<dd>
							 <asp:TextBox ID="txtLogin" runat="server" CssClass="input"></asp:TextBox>
                             <asp:RequiredFieldValidator ValidationGroup="check" ID="RequiredFieldValidator1" runat="server" class="invalid-side-note"
                             ControlToValidate="txtLogin" ErrorMessage="Enter your Login" SetFocusOnError="True" Display="Dynamic">Invalid</asp:RequiredFieldValidator>
							</dd>
							<dt>
								<label>Password <span >*</span></label>
							</dt>
							<dd>
                            <asp:TextBox ID="txtPassword" runat="server" TextMode="Password" CssClass="input"></asp:TextBox>
                              <asp:RequiredFieldValidator ValidationGroup="check" ID="RequiredFieldValidator2" runat="server" class="invalid-side-note"
                                                                        ControlToValidate="txtPassword" ErrorMessage="Enter Password" SetFocusOnError="True"
                                                                        Display="Dynamic">Invalid</asp:RequiredFieldValidator>							
							</dd>
                            <%-- <dt>
                                    <label>
                                    Entity <span>*</span> :
                                    </label>
                                </dt>
                                <dd>
                                <asp:DropDownList ID="ddlEntityList" runat="server" >--%>
                                 <%--<asp:ListItem >Select Entity</asp:ListItem>--%>
                                <%--<asp:ListItem>ICICI Prodential </asp:ListItem>
                                <asp:ListItem>ICICI Life Insurance </asp:ListItem>
                                 <asp:ListItem>HDFC </asp:ListItem>
                                </asp:DropDownList>
                                    <asp:RequiredFieldValidator ID="rfvddlEntityList" runat="server" 
                                        class="invalid-side-note" ControlToValidate="ddlEntityList" Display="Dynamic" 
                                        ErrorMessage="Entity" SetFocusOnError="True" ValidationGroup="a">Invalid</asp:RequiredFieldValidator>
                                </dd>
                                
                                 <dt>
                                --%>      
                                  
					  <u> <a href="ForgotPassword.aspx">Forgot Password</a></u>
                  
						</dl>
                        <asp:Button ID="btnSubmit" Text="Submit" runat="server" ValidationGroup="check" 
                                    onclick="btnSubmit_Click"  />		
                                    <br />	
                           
					</form>
				</div>
				
			</section>
		</article>
        </div> 
  
		<!-- /Full width content box -->
	
